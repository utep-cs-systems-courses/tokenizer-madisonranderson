#include <stdio.h>

int space_char(char c)
{
  if(c == ' ' || c == '\t')
    {
      return 1;
    }
  return 0;
}

int non_space_char(char c)
{
  if(c != ' ' || c != '\t')
    {
      return 1;
    }
  return 0;
}

char *word_start(char *s)
{
   int pointer = 0;
   if(space_char(s[pointer])
   {
     s++;
   }
   while(space_char(s[pointer])
   {
     pointer++;
   }
   return (str++);
}

char *word_terminator(char *word)
{
  char *c = str;
  while(non_space_char(*c) && *c != '\0')
  {
    c++;
  }
  return c;
}

char count_words(char *str)
{
  char *c = str;
  if (word_start(str) == 0)
    {
    }
}
